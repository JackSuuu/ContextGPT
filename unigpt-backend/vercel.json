{
    "version": 2,
    "builds": [
      {
        "src": "api.py",
        "use": "@vercel/python",
        "config": { "installCommand": "pip install --no-cache-dir -r requirements.txt" }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api.py"
      }
    ]
  }
  